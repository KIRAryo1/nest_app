<p id="notice"><%= notice %></p>

<h1>Superclusters</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @superclusters.each do |supercluster| %>
      <tr>
        <td><%= supercluster.name %></td>
        <td><%= link_to 'Show', supercluster %></td>
        <td><%= link_to 'Edit', edit_supercluster_path(supercluster) %></td>
        <td><%= link_to 'Destroy', supercluster, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Supercluster', new_supercluster_path %>
